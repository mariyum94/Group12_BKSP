<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="602.0" prefWidth="771.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.group12_bksp.Mansura.Viewevent">

    <children>
        <Label layoutX="283.0" layoutY="48.0" prefHeight="40.0" prefWidth="180.0" text="MY EVENTS">
            <font>
                <Font size="29.0" />
            </font>
        </Label>

        <TableView fx:id="eventTable" layoutX="36.0" layoutY="156.0" prefHeight="200.0" prefWidth="660.0">
            <columns>
                <TableColumn fx:id="coleventname" prefWidth="172.0" text="eventname" />
                <TableColumn fx:id="coleventdate" prefWidth="146.4" text="eventdate" />
                <TableColumn fx:id="coleventtime" prefWidth="126.4" text="eventtime" />
                <TableColumn fx:id="colvenue" prefWidth="216.8" text="eventvenue" />
            </columns>
        </TableView>

        <Button layoutX="314.0" layoutY="423.0" mnemonicParsing="false" onAction="#onRefreshEvents" prefHeight="26.0" prefWidth="214.0" text="REFRESH" />

        <Button layoutX="314.0" layoutY="493.0" mnemonicParsing="false" onAction="#onBack" prefHeight="26.0" prefWidth="224.0" text="BACK" />
    </children>
</AnchorPane>
