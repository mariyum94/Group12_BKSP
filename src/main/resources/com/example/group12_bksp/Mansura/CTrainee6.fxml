<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.group12_bksp.Mansura.CTrainee6">
    <children>
        <Label layoutX="280.0" layoutY="30.0" prefHeight="35.0" prefWidth="150.0" text="Certificates" textFill="#e70707">
            <font>
                <Font size="22.0" />
            </font>
        </Label>

        <TableView fx:id="certificateTable" layoutX="50.0" layoutY="90.0" prefHeight="350.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="certNameColumn" prefWidth="250.0" text="Certificate Name" />
                <TableColumn fx:id="certDateColumn" prefWidth="150.0" text="Date Issued" />
                <TableColumn fx:id="downloadColumn" prefWidth="150.0" text="Download" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
